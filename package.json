        "cross-env": "^7.0.3",
        "jest": "^27.4.7",
        "nodemon": "^2.0.15",
        "supertest": "^6.2.2"
    },
    "jest": {
        "verbose": false,
        "modulePathIgnorePatterns": [
            ",local"
        ]
